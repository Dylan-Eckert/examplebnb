<%= form_for([@place, @place.reviews.build]) do |f| %>
  <p>
    <%= f.label :reviewer %><br>
    <%= f.text_field :reviewer %>
  </p>
  <p>
    <%= f.label :body %><br>
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.label :rating %><br>
    <%= f.text_area :rating %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
